{
  "app": {
    "title": "Psychia - TSR Therapy Assistant",
    "icon": "ðŸ§ ",
    "language": "pl"
  },
  "agents": {
    "therapist": {
      "provider": "openai",
      "model": "gpt-4o-mini",
      "parameters": {
        "temperature": 0.7,
        "max_tokens": 150,
        "top_p": 0.9
      }
    },
    "supervisor": {
      "provider": "gemini",
      "model": "gemini-2.5-flash-lite",
      "parameters": {
        "temperature": 0.7,
        "max_tokens": 150,
        "top_p": 0.9
      }
    }
  },
  "directories": {
    "logs_dir": "./logs",
    "prompt_dir": "./config/prompts",
    "stages_dir": "./config"
  },
  "memory": {
    "max_therapist_messages": 20,
    "max_supervisor_messages": 15,
    "enable_summary": true,
    "compression_threshold": 100
  },
  "providers": [
    "openai",
    "google"
  ]
}